 qn---------------------------2
 #include <stdio.h>
#include<string.h>

 void main()

 {
     char x[20];
     printf("enter a word");
     gets(x);
     int i=0;
     while(x[i] != '\0')
     {
         if(x[i]>='A' && x[i]<='Z')
         printf("the upper cases are %c",x[i]);
         i++;
     }
     

   
   


 }
 
 
 
