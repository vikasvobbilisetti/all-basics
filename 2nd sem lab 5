question 5th


int main()
{
int num1, num2, choice;

printf("enter 0 forExit\n enter 1 to Add\n enter 2 to Subtract\n enter 3 to Multiply\n enter 4 to Divide");
scanf("%d", &choice);

switch(choice){
    case 0:
        return 0;
        break;
    case 1:
        printf(" 1st number:\n");
        scanf("%d", &num1);
        printf(" 2nd number:\n");
        scanf("%d", &num2);
        printf("%d", add(num1,num2));
        break;
    case 2:
        printf(" 1st number:\n");
        scanf("%d", &num1);
        printf(" 2nd number:\n");
        scanf("%d", &num2);
        printf("%d ", sub(num1,num2));
        break;
    case 3:
        printf(" 1st number:\n");
        scanf("%d", &num1);
        printf("Enter 2nd number:\n");
        scanf("%d", &num2);
        printf("%d", mul(num1,num2));
        break;
    case 4:
        printf(" 1st number:\n");
        scanf("%d", &num1);
        printf("Enter 2nd number:\n");
        scanf("%d", &num2);
        printf("%d", div(num1,num2));
        break;
    default:
        printf("That is not a valid choice.");
        break;
}
}
    
    
int add(int x,int y)
{
    int z = x + y;
    return z;
}   

int sub(int a,int b)
{
    int c = a - b;
    return  c;
}   

int mul(int d,int e)
{
    int f = d * e;
    return f;
    }

int div(int g,int h)
{
    int i = g / h;
    return i;
}

