6th question


#include <stdio.h>

int main()
{
    int num;
    printf("enter a number");
    scanf("%d",&num);
    fact(num);

    
    
}

void fact(int n)
{
    int facto,i;
    facto=1;
    for(i=1;i<=n;i++)
    {
        facto=facto*i;
    }
    printf("%d",facto);
}


