  3rd question
  
  #include<stdio.h>
int main()
{
    int a;
printf("enter the number of the month\n");
scanf("%d", &a);
 month_of_year(a);
return 0;
}

void month_of_year(int b)
{
  if(b==1)
  {
      printf("month is jan");
  }
  else if(b==2)
  {
      printf("month is feb");
  }
  else if(b==3)
  {
      printf("month is march");
  }
  else if(b==4)
  {
      printf("month is april");
  }
  else if(b==5)
  {
      printf("month is may");

  }
  else if(b==6)
  {
      printf("month is june");

  }
  else if(b==7)
  {
      printf("month is july");
  }
  else if(b==8)
  {
      printf("month is aug");
  }
  else if(b==9)
  {
      printf("month is sept");
  }
  else if(b==10)
  {
      printf("month is oct");

  }
  else if(b==11)
  {
      printf("month is nov");
  }
  else if(b==12)
  {
      printf("month is dec");
  }
}


