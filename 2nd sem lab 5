1st question

#include <stdio.h>

int main()
{
    int n;
    printf("enter a number");
    scanf("%d",&n);
    eoo(n);
   
}
  void eoo(int nu)
  {
      if(nu%2==0)
      {
          
          printf("entered number is even");
          
          
      }
      
      else
      {
           printf("entered number is odd");
      }
  }
  
