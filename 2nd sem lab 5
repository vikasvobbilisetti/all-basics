question 4th


#include <stdio.h>

int main()
{
    printf("enter three numbers ");
    int i,j,k;
    scanf("%d ",&i);
    scanf("%d ",&j);
    scanf("%d ",&k);
    gre(i,j,k);
    

   
}
void gre(int a, int b, int c)
{
    if(a>=b && a>=c)
    {
        printf("a is the largest number");
        
    }
    else if(b>=a && b>=c)
    {
        printf("b is the largest number");
        
    }
    
     else if(c>=a && c>=b)
    {
        printf("c is the largest number");
        
    }
}


