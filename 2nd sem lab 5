8th question


#include <stdio.h>
#include <stdlib.h>

int main()
{
     int n,flag;
    printf ("enter a number: ");
    scanf ("%d",&n);
    flag= prime (n);
     if (flag==2)
    {
        printf("%d is prime number",n);
    }
    else
    {
        printf("%d is not prime number",n);
    }

}
 int prime (int x)
 {   int count=0,i;
     for (i=1;i<=x;i++)
    {
        if (x%i==0)
        {
            count=count+1;
        }
    }
    return count;
 }


