  #include <stdio.h>

int main()
{
    int n,y;
    printf("enter a number");
    scanf("%d",&n);
    y=sq(n);
    printf("%d is the square",y);
    
    
   
}
  int sq(int nu)
  {
      int x;
      x=nu*nu;
      return x;
  }
