question-1
#include <stdio.h>
#include <stdlib.h>

int main()
{
  int n,i,j,k;
  printf ("enter the size of the array: ");
  scanf ("%d",&n);
  int a[n],b[n],c[n];
  printf ("enter the array elements: ");
      j=0;
      k=0;


  for (i=0;i<=n-1;i++)
  {

      scanf ("%d",&a[i]);

  }

  for (i=0;i<=n-1;i++)
  {

     if (a[i]%2==0)
     {
         b[j]=a[i];
         j=j+1;
     }
     else
     {
        c[k]=a[i];
        k=k+1;
     }
  }
  printf ("even array elements are: \n");
  for (i=0;i<=j-1;i++)
  {
      printf ("%d\n",b[i]);
  }
  printf ("odd array elements are: \n");
  for (i=0;i<=k-1;i++)
  {
      printf ("%d\n",c[i]);
  }
    return 0;
}





