question -18


[8:49 AM, 5/28/2021] Koti: #include <stdio.h>
#include <stdlib.h>

int main()
{
    int r,c,i,j,k;
    printf ("enter the no of rows of matrices\n: ");
    scanf ("%d",&r);

    printf ("enter the no of columns of matrices\n: ");
    scanf ("%d",&c);



      int m1[r][c];

      int m2[r][c];

      int m[r][c];


        printf ("enter the first matrix\n");

       for (i=0;i<=r-1;i++)

       {
         for (j=0;j<=c-1;j++)
         {
             scanf ("%d",&m1[i][j]);
         }

       }

       printf ("enter the second matrix\n");

       for (i=0;i<=r-1;i++)

       {
         for (j=0;j<=c-1;j++)
         {
             scanf ("%d",&m2[i][j]);
         }
       }

    printf("resultant multiplication of matrix:\n");


      for (i=0;i<=r-1;i++)

       {
         for (j=0;j<=c-1;j++)

      {
          m[i][j]=0;

             for (k=0;k<=c-1;k++)
         {
           m[i][j]=m[i][j]+m1[i][k]*m2[k][j];
         }
       }
       }


         for (i=0;i<=r-1;i++)

       {
         for (j=0;j<=c-1;j++)
         {
           printf ("\t%d",m[i][j]);
         }
         printf ("\n");
       }




    return 0;
}





question -16



#include <stdio.h>
#include <stdlib.h>

int main()
{
   int r,c,i,j,sum,flag=0;
    printf ("enter the no of rows of matrices\n: ");
    scanf ("%d",&r);

    printf ("enter the no of columns of matrices\n: ");
    scanf ("%d",&c);

      int m[r][c];



        printf ("enter the values of  matrix\n");

       for (i=0;i<=r-1;i++)

       {
         for (j=0;j<=c-1;j++)
         {
             scanf ("%d",&m[i][j]);
         }

       }

	for (i=1;i<=r-1;i++)
        {
		for (j=0;j<=c-1;j++)
        {


			if (m[i][j]!= 0)
            {
				flag = 0;
            }

        }
        }

	if (flag=1)
        {
		printf("the matrix is upper triangular\n");
        }
	else
    {
        printf(" the matrix is not upper triangular\n");
    }

     sum=0;
     printf ("sum of the elements of the matrix is:");

     for (i=0;i<=r-1;i++)
     {
         for (j=0;j<=c-1;j++)
            sum=sum+m[i][j];
     }
     printf ("%d",sum);
    return 0;
}


