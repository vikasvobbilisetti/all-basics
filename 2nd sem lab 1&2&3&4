\\4th question.


#include <stdio.h>
#include <stdlib.h>

int main()
{
  int n,i;
  printf ("enter the size of the array: ");
  scanf ("%d",&n);
  int a[n];
  printf ("enter the array elements: ");
  for (i=0;i<=n-1;i++)
  {

      scanf ("%d",&a[i]);

  }
  printf ("the array elements in reverse order are: ");
  for (i=n-1;i>=0;i--)
  {
      printf ("%d  ",a[i]);
  }

    return 0;
}


5th question.


#include <stdio.h>
#include <stdlib.h>

int main()
{
  int n,i,sum,pro;
  printf ("enter the size of the array: ");
  scanf ("%d",&n);
  sum=0;
  pro=1;
  int a[n];
  printf ("enter the array elements: ");
  for (i=0;i<=n-1;i++)
  {

      scanf ("%d",&a[i]);

  }

  for (i=0;i<=n-1;i++)
  {
      sum=sum+a[i];
      pro=pro*a[i];
  }
   printf ("the  sum of array elements is %d\n ",sum);
      printf ("the product of array elements is %d ",pro);
    return 0;
}


6th question..



#include <stdio.h>
#include <stdlib.h>

int main()
{
  int n,i,x,y,count,flag;
  printf ("enter the size of the array: ");
  scanf ("%d",&n);
  int a[n];
  printf ("enter the array elements: ");
  count=0;
  flag=0;
  for (i=0;i<=n-1;i++)
  {

      scanf ("%d",&a[i]);

  }

  for (i=0;i<=n-1;i++)
  {
       x=a[i]%10;
       y=a[i]%3;


     if (x==0)
     {
         count=count+1;
     }
     if (y==0)
     {
         flag=flag+1;
     }


  }
  printf ("the count of numbers which are ending with zero are: %d\n",count);
printf ("the count of numbers which are divisible by three are: %d",flag);


    return 0;
}



