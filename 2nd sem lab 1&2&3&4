question --9

#include <stdio.h>
#include <stdlib.h>

int main()
{
    int r,c,i,j,sum;
    printf ("enter the no of rows: ");
    scanf ("%d",&r);
    printf ("enter the no of columns: ");
    scanf ("%d",&c);
    int m[i][j];
    for (i=0;i<=r-1;i++)
    {
        for (j=0;j<=c-1;j++)
        {
            scanf ("%d",&m[i][j]);
        }
    }

    for (j=0;j<=c-1;j++)
    {   sum=0;
        for (i=0;i<=r-1;i++)
        {
            sum=sum+m[i][j];
        }
        printf ("the sum of %d column is %d\n",j,sum);

    }
    return 0;
}

