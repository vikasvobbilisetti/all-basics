8th question.



#include <stdio.h>
#include <stdlib.h>

int main()
{
  int n,i,min,max;
  printf ("enter the size of the array: ");
  scanf ("%d",&n);
  int a[n];
  printf ("enter the array elements: ");
  for (i=0;i<=n-1;i++)
  {

      scanf ("%d",&a[i]);

  }
   min=max=a[0];
   for (i=1;i<=n-1;i++)
   {
       if (min>a[i])
       {
           min=a[i];
       }
       if (max<a[i])
       {
           max=a[i];
       }
   }
   printf ("minimum element in the array is %d\n ",min);
    printf ("maximum element in the array is %d ",max);

    return 0;
}



9th question.



#include <stdio.h>
#include <stdlib.h>

int main()
{
  int n,i,key,count;
  printf ("enter the size of the array: ");
  scanf ("%d",&n);
  int a[n];
  printf ("enter the array elements: ");

       for (i=0;i<=n-1;i++)
      {

      scanf ("%d",&a[i]);

      }


  printf ("enter the key element to search: ");
  scanf ("%d",&key);
  count=0;

       for (i=0;i<=n-1;i++)
      {
             if (key==a[i])
            {
                 count=count+1;
                  printf ("the position at which the element is occuring is %d\n ",i);
            }
      }
      printf ("the number of times the element is occuring is %d  ",count);

    return 0;
}



10th question.


#include <stdio.h>
#include <stdlib.h>

int main()
{
  int n,i,j,min,temp;
  printf ("enter the size of the array: ");
  scanf ("%d",&n);
  int a[n];
  printf ("enter the array elements: ");
  for (i=0;i<=n-1;i++)
  {

      scanf ("%d",&a[i]);

  }

  for (i=0;i<n-1;i++)
  {

      min=i;


     for (j=i+1;j<=n-1;j++)
     {
         if (a[j]<a[min])
         {
             min=j;
         }
     }
      temp=a[i];
      a[i]=a[min];
      a[min]=temp;



  }
  for (i=0;i<=n-1;i++)
  {
      printf ("%d  ",a[i]);
  }

    return 0;
}


11th question.



#include <stdio.h>
#include <stdlib.h>

int main()
{
  int n,i,j,temp;
  printf ("enter the size of the array: ");
  scanf ("%d",&n);
  int a[n];
  printf ("enter the array elements: ");
  for (i=0;i<=n-1;i++)
  {

      scanf ("%d",&a[i]);

  }
  for (i=0;i<=n-1;i++)
    {
        for (int j=0;j<n-1;j++)
        {

               if (a[j]>a[j+1])
              {
                    temp=a[j];
                    a[j]=a[j+1];
                    a[j+1]=temp;
              }


        }
    }


    for(i=0;i<=n-1;i++)
    {
        printf("%d  ",a[i]);
    }
    return 0;
}



12th question..



#include <stdio.h>
#include <stdlib.h>

int main()
{
  int n,i,j,count;
  printf ("enter the size of the array: ");
  scanf ("%d",&n);
  int a[n];
  printf ("enter the array elements: ");
  for (i=0;i<=n-1;i++)
  {

      scanf ("%d",&a[i]);

  }

  for (i=0;i<=n-1;i++)
  {
       count=0;

     for (j=0;j<=n-1;j++)
     {
               if (a[i]==a[j])
              {
                 count=count+1;
              }

     }
     printf ("%d occurs %d times\n",a[i],count);

  }



    return 0;
}





