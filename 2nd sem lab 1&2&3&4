question -14


#include <stdio.h>
#include <stdlib.h>

int main()
{
    int r,c,i,j;
    printf ("enter the no of rows of matrices\n: ");
    scanf ("%d",&r);

    printf ("enter the no of columns of matrices\n: ");
    scanf ("%d",&c);

      int m[r][c];
      int k;


        printf ("enter the values of  matrix\n");

       for (i=0;i<=r-1;i++)

       {
         for (j=0;j<=c-1;j++)
         {
             scanf ("%d",&m[i][j]);
         }

       }
       printf ("enter the scalar multiple: ");
       scanf("%d",&k);

          for (i=0;i<=r-1;i++)

         {
         for (j=0;j<=c-1;j++)
         {
            m[i][j]= k * m[i][j];
         }
         }

         printf ("\n resultant matrix\n");


         for (i=0;i<=r-1;i++)

         {
             for (j=0;j<=c-1;j++)
            {
             printf("%d \t ",m[i][j]);
             printf ("\n");
            }

         }

    return 0;

}

