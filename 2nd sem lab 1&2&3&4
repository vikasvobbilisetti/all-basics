question-15


#include <stdio.h>
#include <stdlib.h>

int main()
{
    int r,c,i,j,flag;
    printf ("enter the no of rows of matrices\n: ");
    scanf ("%d",&r);

    printf ("enter the no of columns of matrices\n: ");
    scanf ("%d",&c);

      int m[r][c];



        printf ("enter the values of  matrix\n");

       for (i=0;i<=r-1;i++)

       {
         for (j=0;j<=c-1;j++)
         {
             scanf ("%d",&m[i][j]);
         }

       }

         for (i=0;i<=r-1;i++)
         {
             for (j=0;j<=c-1;j++)
             {
                 if (i<j)
                 {
                     flag=m[i][j];
                     m[i][j]=m[j][i];
                     m[j][i]=flag;
                 }
             }
         }


  printf ("\n the transpose of the matrix is: \n");

         for (i=0;i<=r-1;i++)
         {
             for (j=0;j<=c-1;j++)
             {
                 printf ("%d\t",m[i][j]);
             }
             printf ("\n");
         }
    return 0;
}


