question-4
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int r,c,i,j;
    printf ("enter the no of rows: ");
    scanf ("%d",&r);
    printf ("enter the no of columns: ");
    scanf ("%d",&c);

    int m[i][j];

    printf ("enter the values of matrices: \n");

    for (i=0;i<=r-1;i++)
    {
        for (j=0;j<=c-1;j++)
        {
            scanf ("%d",&m[i][j]);
        }
    }
     printf ("\n final matrix is \n");

      for (i=0;i<=r-1;i++)
    {
        for (j=0;j<=c-1;j++)
        {
             printf("\t %d",m[i][j]);
        }
           printf("\n");
    }


  return 0;
}
