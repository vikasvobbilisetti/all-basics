question-6



#include <stdio.h>
#include <stdlib.h>

int main()
{
    int r1,c1,r2,c2,i,j;
    printf ("enter the no of rows of first matrix\n: ");
    scanf ("%d",&r1);

    printf ("enter the no of columns of first matrix\n: ");
    scanf ("%d",&c1);

    printf ("enter the no of rows of second matrix\n: ");
    scanf ("%d",&r2);

    printf ("enter the no of rows of second matrix\n: ");
    scanf ("%d",&c2);

    if (r1==r2 && c1==c2)
    {
      int m1[r1][c1];

      int m2[r2][c2];

      int r[r2][c2];


        printf ("enter the first matrix\n");

       for (i=0;i<=r1-1;i++)

       {
         for (j=0;j<=c1-1;j++)
         {
             scanf ("%d",&m1[i][j]);
         }

       }

       printf ("enter the second matrix\n");

       for (i=0;i<=r2-1;i++)

       {
         for (j=0;j<=c2-1;j++)
         {
             scanf ("%d",&m2[i][j]);
         }
       }

      printf ("resultant matrix\n");

        for (i=0;i<=r1-1;i++)

       {
         for (j=0;j<=c1-1;j++)
         {
           r[i][j]= m1[i][j]+m2[i][j];
         }
       }


         for (i=0;i<=r1-1;i++)

       {
         for (j=0;j<=c1-1;j++)
         {
           printf ("\t%d",r[i][j]);
         }
         printf ("\n");
       }



    }
    else
    {
        printf ("there is no matrix addition");
    }





    return 0;
}







